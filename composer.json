{
    "name": "agoat/contao-languagerelation",
    "description": "Contao 4 Lanugagerelation",
    "keywords": ["contao", "contao4", "contao-bundle", "plugin", "content", "language", "multilingual", "translation", "cms", "management"],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Arne Stappen (alias aGoat)",
            "homepage": "https://agoat.xyz"
        }
    ],
    "support": {
        "issues": "https://github.com/agoat/contao-languagerelation-bundle/issues",
        "source": "https://github.com/agoat/contao-languagerelation-bundle"
    },
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "4.4.*",
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "suggest": {
        "agoat/contao-permalink": "Clear and easy url handling for multilingual pages"
    },
    "autoload": {
        "psr-4": {
            "Agoat\\LanguageRelationBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Agoat\\LanguageRelationBundle\\ContaoManager\\Plugin"
    }
}
